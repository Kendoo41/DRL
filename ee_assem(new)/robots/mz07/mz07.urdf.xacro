                    ax="0" ay="0" az="1"
                    el="30.0"
                    ll="${J1_lower_limit}" ul="${J1_upper_limit}"
                    vl="${J1_velocity_limit}"
                    skp="1" skv="1"
                    sll="${J1_lower_limit}" sul="${J1_upper_limit}"/>

